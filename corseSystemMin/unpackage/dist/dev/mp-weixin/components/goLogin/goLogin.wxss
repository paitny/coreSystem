.container {
  margin: 0px;
  position: fixed;
  width: 100%;
  height: 100%;
  background: url(https://wypty.cn/static//file/material/bgImg.png) no-repeat;
  background-size: 100% 100%;
}
.container .bg_top {
  height: 70%;
  width: 100%;
}
.container .bg_top image {
  width: 320rpx;
  height: 320rpx;
  margin: 25px 29% 0px;
}
.container .bg_top .text {
  margin: 2% 17%;
  text-align: center;
}
.container .bg_top .text .name {
  font-family: "幼圆";
  font-size: 30px;
  font-weight: bold;
}
.container .bg_top .text .name .p {
  font-family: "幼圆";
  font-size: 9px;
  font-weight: 400;
}
.container .bg_top .text .intro {
  margin: 10px;
  font-size: 12px;
  color: #a7a5aa;
}
.container .bg_top .text .intro .p {
  font-family: "幼圆";
  font-size: 9px;
  font-weight: 100;
}
.container .btn button {
  width: 70%;
  height: 90rpx;
  color: #b8b2cc;
  font-size: 16px;
  border-radius: 80rpx;
  background-color: #5c55f7;
  border: 2px solid #b8b2cc;
}
.container .btn .login {
  line-height: 90rpx;
}
.container .btn .register {
  width: 70%;
  height: 90rpx;
  font-size: 16px;
  background-color: #fff;
  border: 2px solid #b8b2cc;
  margin-top: 40rpx;
  line-height: 90rpx;
}
.container .btn .agreement {
  margin: 20px 20% 0px 20%;
  height: 30px;
}
.container .btn .agreement .agreement_text {
  float: right;
  width: 85%;
  font-family: "幼圆";
  font-size: 14px;
}
.container .btn .agreement .agreement_text .pp {
  display: inline;
}
.container .btn .agreement .agreement_text .aa {
  color: #3580BB;
  display: inline;
}