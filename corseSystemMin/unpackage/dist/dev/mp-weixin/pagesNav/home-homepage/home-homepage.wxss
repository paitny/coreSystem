.container {
  width: 100vw;
  height: 100vh;
  display: flex;
  justify-content: center;
  /* 水平居中 */
  align-items: center;
  /* 垂直居中 */
}
.container .button-one {
  height: 200rpx;
  margin-top: 25rpx;
  position: relative;
}
.container .button-one .title {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  font-size: 45rpx;
  color: #fff;
  font-family: "Roboto", sans-serif;
}
.container .button-one image {
  width: 400rpx;
  height: 200rpx;
  border-radius: 30rpx;
}
page {
