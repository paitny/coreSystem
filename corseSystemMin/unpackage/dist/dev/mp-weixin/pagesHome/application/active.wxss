.floating-button {
  position: fixed;
  bottom: 20px;
  right: 20px;
  background: #007AFF;
  color: white;
  padding: 15px 15px;
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2);
  text-align: center;
  z-index: 999999;
}
.container {
  height: 1700rpx;
  width: 100%;
}
.container .function {
  display: flex;
  padding: 0rpx 50rpx 15rpx 50rpx;
  justify-content: space-between;
  background-color: #fff;
}
.container .function .deadline {
  background-color: #fff;
  font-size: 25rpx;
  margin-top: 10rpx;
}
.container .function .delete image {
  width: 50rpx;
  height: 50rpx;
  background-color: #fff;
}
.container .frame {
  width: 100%;
  background-color: white;
  font-size: 33rpx;
  text-align: center;
  padding: 40rpx 0 40rpx 0;
}
.container .content {
  margin-top: 5rpx;
  width: 100%;
  background-color: white;
  padding-bottom: 50rpx;
}
.container .content .cont {
  height: 100%;
  width: 90%;
  margin: 0 auto;
}
.container .content .cont .box {
  margin-top: 83rpx;
  height: 95rpx;
  width: 100%;
  background-color: #E6E6E6;
}
.container .content .cont .submit {
  height: 80rpx;
  width: 100%;
  margin-top: 50rpx;
  background-color: #4d6398;
  line-height: 75rpx;
  color: white;
  font-size: 32rpx;
  border-radius: 50rpx;
}